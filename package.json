{
  "name": "find-apartment-react",
  "version": "1.0.0",
  "description": "Full stack react and express app",
  "main": "server.js",
  "jest": {
    "testPathIgnorePatterns": [
      "cypress",
      "client"
    ]
  },
  "scripts": {
    "start": "nodemon --inspect server.js",
    "server_start": "NODE_ENV=production node server.js",
    "start_scrape_server": "nodemon scrapeServer.js",
    "test": "jest --watch",
    "client_start": "npm start --prefix client",
    "client_install": "npm install --prefix client",
    "client_test": "npm test --prefix client",
    "client_build": "npm run build --prefix client",
    "dev": "concurrently npm:server_start npm:client_start",
    "cypress:open": "cypress open",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "configure": ".bin/configuredb.sh"
  },
  "author": "Zach Seaman",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bottleneck": "^2.19.5",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "crypto-js": "^3.1.9-1",
    "cypress": "^3.8.3",
    "date-fns": "^1.30.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-craigslist": "^2.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "polygon-lookup": "^2.5.0",
    "sequelize": "^5.8.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "concurrently": "^5.0.0",
    "jest": "^25.1.0",
    "nodemon": "^1.19.1",
    "pm2": "^4.2.3",
    "prettier": "^1.19.1",
    "test-data-bot": "^0.8.0"
  }
}
